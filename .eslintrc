{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "spaced-comment":  "off",
    "quotes": "off",
    "operator-linebreak" : "off",
    "no-trailing-spaces": "off",
    "require-jsdoc": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}